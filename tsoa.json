{
  "entryFile": "./src/template/api/router.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": ["./src/template/api/*.ts"],
  "spec": {
    "outputDirectory": "./dist/core/api",
    "specVersion": 3,
    "yaml": true,
    "json": true,
    "specFileBaseName": "openapi"
  },
  "routes": {
    "routesDir": "./dist/core/api"
  },
  "configuration": {
    "resolveModuleNames": {
      "swagger-ui-express": {
        "default": "swaggerUiExpress"
      }
    }
  }
}