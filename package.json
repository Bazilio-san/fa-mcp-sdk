{
  "name": "fa-mcp-sdk",
  "productName": "FA MCP SDK",
  "version": "0.2.279",
  "description": "Core infrastructure and templates for building Model Context Protocol (MCP) servers with TypeScript",
  "type": "module",
  "main": "dist/core/index.js",
  "types": "dist/core/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist/core/**/*",
    "cli-template/**/*",
    "cli-template/.gitignore",
    "bin/**/*",
    "config/**/*",
    "src/template/**/*",
    "src/tests/**/*",
    "scripts/**/*",
    "README.md"
  ],
  "bin": {
    "fa-mcp": "./bin/fa-mcp.js"
  },
  "scripts": {
    "start": "npm run template:start",
    "build": "tsc && npm run copy-static",
    "copy-static": "node scripts/copy-static.js",
    "clean": "rimraf dist",
    "cb": "npm run clean && npm run build",
    "dev": "tsc --watch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "ci": "node --no-deprecation ./scripts/npm/run.js",
    "reinstall": "node --no-deprecation ./scripts/npm/run.js reinstall",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run cb",
    "template:start": "node dist/template/start.js",
    "template:stdio": "node dist/template/start.js stdio",
    "token-gen": "node dist/core/auth/token-generator/server.js",
    "tsoa:spec": "tsoa spec"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "typescript",
    "server",
    "framework",
    "template",
    "claude",
    "ai",
    "api",
    "prompts",
    "tools",
    "resources"
  ],
  "author": {
    "name": "Viacheslav Makarov",
    "email": "npmjs@bazilio.ru"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Bazilio-san/fa-mcp-sdk.git"
  },
  "homepage": "https://github.com/Bazilio-san/fa-mcp-sdk#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@tsoa/runtime": "^6.6.0",
    "af-ad-ts": "^0.0.69",
    "af-db-ts": "^3.0.24",
    "af-logger-ts": "^0.0.26",
    "chalk": "^5.6.2",
    "config": "^4.2.0",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "fa-consul": "^1.0.7",
    "helmet": "^8.1.0",
    "js-yaml": "^4.1.1",
    "node-cache": "^5.1.2",
    "openai": "^6.18.0",
    "pgvector": "^0.2.1",
    "rate-limiter-flexible": "^9.1.0",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^6.6.0",
    "uuid": "^13.0.0",
    "ya-express-ntlm": "^1.0.39"
  },
  "devDependencies": {
    "@tsoa/cli": "^6.6.0",
    "@types/config": "^3.3.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/js-yaml": "^4.0.9",
    "@types/mssql": "^9.1.9",
    "@types/node": "^25.2.1",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.39.2",
    "eslint-config-at-26": "^25.12.23",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3"
  }
}
